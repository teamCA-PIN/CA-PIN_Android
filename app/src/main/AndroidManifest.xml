<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.caffeine.capin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".CapinApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Capin"
        android:usesCleartextTraffic="true">
        <activity android:name=".mypage.MyPageActivity"></activity>
        <activity android:name=".mypage.pin.MyPagePinDetailActivity" />
        <activity android:name=".mypage.MyPageProfileEditActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>